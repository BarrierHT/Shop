<main>

    <form class="login-form" action="/login" method="POST"> 
        <div>
            <h3>Insert your User</h3>
        </div>
        
        <div class="form-control">
            <label for="email">Email</label> 
            <input type="email " name="email"  placeholder="Write the email" id="email">
        </div>
        <div class="form-control">
            <label for="password">Password</label> 
            <input type="password" name="password"  placeholder="Write the password" id="password">
        </div>

        <button class="btn" type="submit">Login</button>
    </form> 



</main>

